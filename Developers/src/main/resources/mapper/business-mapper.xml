<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="business">
	<insert id="insertEmployer" parameterType="map" >
		INSERT INTO USERTEMP VALUES (#{userName}, #{userPhone}, #{userEmail}, #{userPassword}, DEFAULT)
	</insert>
	<insert id="insertBusiness" parameterType="com.kh.developers.business.model.vo.Business">
		INSERT INTO BUSINESS_INFO VALUES (BUSSEQ.NEXTVAL,#{busName},#{busCountry},#{busArea},#{busAdress},#{busRegNum},#{busIncome},#{busIndustrial},#{busTotalEmp},#{busIntroduce},#{busEstablishment},#{busEmail},#{busPhone},#{busWebsite},#{searchKeywords},#{regPath},DEFAULT)
	</insert>
</mapper>
